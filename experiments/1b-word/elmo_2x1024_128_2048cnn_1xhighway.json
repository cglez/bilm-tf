{
  "bidirectional": true,

  "char_cnn": {
    "activation": "relu",
    "filters": [
      [1, 32],
      [2, 32],
      [3, 64],
      [4, 128],
      [5, 256],
      [6, 512],
      [7, 1024]
    ],
    "max_characters_per_token": 50,
    "embedding": {"dim": 16},
    "n_characters": 261,
    "n_highway": 1
